<%= form_for(@song) do |f| %>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :writeup %><br>
    <%= f.text_area :writeup %>
  </div>
  <div class="field">
    <%= f.label :mp3 %><br>
    <%= f.file_field :mp3 %>
  </div>
  <div class="field">
    <%= f.label :chords %><br>
    <%= f.file_field :chords %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
